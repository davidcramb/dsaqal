{
  "manifest_version": 2,

  "name": "DSA QAL",
  "description": "This extension is meant for use with DSA for **** Sales Reps",
  "version": "0.01",


  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Config and Settings"
  },
  "content_scripts": {
    "matches": ["*://*.sales.dell.com/*"],
    "js": ["scripts.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}